{
  "expo": {
    "name": "Ping",
    "owner": "zaniluca",
    "description": "Multiplatform react-native app that sends you instant notifications about gitlab activities",
    "slug": "ping4gitlab",
    "version": "2.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "githubUrl": "https://github.com/zaniluca/ping-4-gitlab",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": false,
      "bundleIdentifier": "com.zaniluca.ping4gitlab",
      "buildNumber": "31",
      "config": {
        "usesNonExemptEncryption": false
      },
      "appStoreUrl": "https://apps.apple.com/app/ping-for-gitlab/id1620904531"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.zaniluca.ping4gitlab",
      "versionCode": 19,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.zaniluca.ping4gitlab"
    },
    "plugins": [
      "sentry-expo",
      [
        "expo-notifications",
        {
          "sounds": ["./assets/notification.wav"],
          "icon": "./assets/notification-icon.png",
          "color": "#E24329",
          "androidMode": "collapse",
          "androidCollapsedTitle": "{unread_notifications} Notifications from Gitlab"
        }
      ]
    ]
  }
}
