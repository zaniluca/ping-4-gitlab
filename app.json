{
  "expo": {
    "name": "Gitlab Ping",
    "owner": "zaniluca",
    "scheme": "ping4gitlab",
    "description": "Multiplatform react-native app that sends you instant notifications about gitlab activities",
    "slug": "ping4gitlab",
    "version": "3.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "githubUrl": "https://github.com/zaniluca/ping-4-gitlab",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/bdcab203-d9ce-48aa-95db-938dbfe1be17"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.zaniluca.ping4gitlab",
      "config": {
        "usesNonExemptEncryption": false
      },
      "appStoreUrl": "https://apps.apple.com/app/ping-for-gitlab/id1620904531",
      "associatedDomains": ["webcredentials:pfg.app"]
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/android-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.zaniluca.ping4gitlab",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.zaniluca.ping4gitlab",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "ping4gitlab"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      "expo-localization",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "sounds": ["./assets/notification.wav"],
          "icon": "./assets/notification-icon.png",
          "color": "#E24329",
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": false
    }
  }
}
