{
  "expo": {
    "name": "Ping",
    "owner": "zaniluca",
    "slug": "ping4gitlab",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/bdcab203-d9ce-48aa-95db-938dbfe1be17"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "supportsTablet": false,
      "bundleIdentifier": "com.zaniluca.ping4gitlab",
      "buildNumber": "10",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.zaniluca.ping4gitlab",
      "versionCode": 8
    },
    "plugins": [
      "sentry-expo",
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/notification.wav"
          ],
          "icon": "./assets/notification-icon.png",
          "color": "#E24329",
          "androidMode": "collapse",
          "androidCollapsedTitle": "{unread_notifications} Notifications from Gitlab"
        }
      ]
    ]
  }
}
