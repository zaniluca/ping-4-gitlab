{
  "cli": {
    "version": ">= 0.49.0"
  },
  "build": {
    "base": {
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true,
        "cocoapods": "1.11.2"
      }
    },
    "development": {
      "extends": "base",
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": true,
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "channel": "master",
      "distribution": "internal",
      "extends": "base",
      "android": {
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "simulator": false,
        "buildConfiguration": "Debug"
      },
      "env": {
        "API_URL": "https://api-ping-4-gitlab-staging.up.railway.app/"
      }
    },
    "staging": {
      "channel": "master",
      "extends": "base",
      "env": {
        "API_URL": "https://api-ping-4-gitlab-staging.up.railway.app/"
      }
    },
    "production": {
      "channel": "production",
      "extends": "base",
      "env": {
        "API_URL": "https://api-ping-4-gitlab-production.up.railway.app/"
      }
    }
  },
  "submit": {
    "base": {
      "ios": {
        "ascAppId": "1620904531",
        "appleTeamId": "R263JVGPBB"
      }
    },
    "production": {
      "extends": "base"
    },
    "staging": {
      "extends": "base"
    }
  }
}
